%token NUMBER STRING JOSI WORD COMMENT

%left '+' '-'
%left '*' '/'
%left '^'

%%
program : lines { return $1; }
        ;

lines : { $$ = '';}
	| lines line { $$ = $1 + $2+ '\n'; }
	;

line : NUMBER { $$ = "N:" + $1 + '\n';}
	| WORD { $$ = "W:" + $1 + '\n'; }
	| STRING { $$ = "S:" + $1 + '\n'; }
	;

%%
